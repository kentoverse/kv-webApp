    "firebase-functions": "^3.23.0",
    "next": "13.3.0",
    "postcss": "8.4.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.1",
    "typescript": "5.0.4",
    "firebase-frameworks": "^0.6.0",
    "firebase-admin": "^11.0.1"
  },
  "main": "server.js"
